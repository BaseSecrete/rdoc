<%- if klass.type == 'class' then %>
  <div class="nav-section">
    <h3>Parent</h3>
    <ul class="link-list">
      <%- if klass.superclass and not String === klass.superclass then -%>
        <li><a href="<%= klass.aref_to klass.superclass.path %>"><%= klass.superclass.full_name %></a>
      <%- else -%>
        <li><%= klass.superclass %>
      <%- end -%>
    </ul>
  </div>
<%- end -%>
